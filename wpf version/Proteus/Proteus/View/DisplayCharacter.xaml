<Window x:Class="Proteus.View.DisplayCharacter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proteus.View"
        mc:Ignorable="d"
        Title="DisplayCharacter" Height="300" Width="300">
    <StackPanel>
        <Label HorizontalAlignment="Center"> - Charactor Info - </Label>
        <TextBox Name="MainTextBox" Height="220" IsReadOnly="True" TextChanged="MainTextBox_TextChanged" Margin="10">
         
        </TextBox>
    </StackPanel>
</Window>
